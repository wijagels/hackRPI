{
  "manifest_version": 2,
  "name": "Myo For Chrome",
  "description": "Brah",
  "version": "1.0",
  
  "permissions": [ "tabs", "background", "storage" ],
  "options_page": "options.html",
  "content_scripts": [ {
    "js": [ "myo.js", "scroll.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "background": {
    "scripts": ["myo.js", "background.js"]
  }
}
